{
  "name": "cimc-website",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "defaults"
  ],
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
